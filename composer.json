{
    "name": "hasura-extra/api-client",
    "license": "MIT",
    "description": "The library provides an api client to interact with Hasura api.",
    "prefer-stable": true,
    "minimum-stability": "dev",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Hasura\\ApiClient\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hasura\\ApiClient\\Tests\\": "tests"
        }
    },
    "require": {
        "php": ">=8.0",
        "symfony/http-client": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    }
}
